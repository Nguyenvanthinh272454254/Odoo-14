<odoo>
    <record model="ir.ui.view" id="jobpositions_tree_view">
        <field name="name">jobpositions list</field>
        <field name="model">jobpositions</field>
        <field name="arch" type="xml">
          <tree>
            <field name="name"/>
          </tree>
        </field>
      </record>


    <record model="ir.actions.act_window" id="actions_jobpositions">
        <field name="name">jobpositions window</field>
        <field name="res_model">jobpositions</field>
        <field name="view_mode">tree,form</field>
      </record>
</odoo>