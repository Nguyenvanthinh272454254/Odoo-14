<odoo>
    <record model="ir.ui.view" id="setting_tree_view">
        <field name="name">setting list</field>
        <field name="model">setting</field>
        <field name="arch" type="xml">
          <tree>
            <field name="name"/>
          </tree>
        </field>
      </record>


    <record model="ir.actions.act_window" id="actions_setting">
        <field name="name">setting window</field>
        <field name="res_model">setting</field>
        <field name="view_mode">tree,form</field>
      </record>
</odoo>